{
  "name": "@questline/game-logic",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./werewolf": {
      "import": "./dist/werewolf/index.mjs",
      "require": "./dist/werewolf/index.js",
      "types": "./dist/werewolf/index.d.ts"
    },
    "./ttrpg": {
      "import": "./dist/ttrpg/index.mjs",
      "require": "./dist/ttrpg/index.js",
      "types": "./dist/ttrpg/index.d.ts"
    },
    "./pomodoro": {
      "import": "./dist/pomodoro/index.mjs",
      "require": "./dist/pomodoro/index.js",
      "types": "./dist/pomodoro/index.d.ts"
    },
    "./dice": {
      "import": "./dist/dice/index.mjs",
      "require": "./dist/dice/index.js",
      "types": "./dist/dice/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@questline/types": "*"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
